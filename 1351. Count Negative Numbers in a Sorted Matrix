class Solution {
public:
    int countNegatives(vector<vector<int>>& g) {
        int c=0;
        int m=g[0].size();
        for(int i=0; i<g.size();i++){
            for(int j=0; j<g[0].size(); j++){
                if(g[i][j]<0){
                    c=c+1;
                }
            }
            
        }
        return c;
    }
};
